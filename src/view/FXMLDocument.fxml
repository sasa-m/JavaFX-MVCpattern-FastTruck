<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="main_pane" prefHeight="500.0" prefWidth="1000.0" style="-fx-background-color: #bdc3c7;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.FXMLDocumentController">
    <children>
        <Button fx:id="bt_main_close" layoutX="856.0" layoutY="448.0" onAction="#closeButtonAction" prefHeight="45.0" prefWidth="140.0" text="Close">
         <font>
            <Font name="Tahoma" size="20.0" />
         </font></Button>
      <ImageView fx:id="image_main" fitHeight="430.0" fitWidth="990.0" layoutX="5.0" layoutY="8.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/FistPage2.jpg" />
         </image>
      </ImageView>
      <Button fx:id="bt_main_trucks" layoutX="706.0" layoutY="448.0" mnemonicParsing="false" onAction="#trucksButtonAction" prefHeight="45.0" prefWidth="140.0" text="Trucks">
         <font>
            <Font name="Tahoma" size="20.0" />
         </font>
      </Button>
      <Button fx:id="bt_main_drivers" layoutX="557.0" layoutY="448.0" mnemonicParsing="false" onAction="#driversButtonAction" prefHeight="45.0" prefWidth="140.0" text="Drivers">
         <font>
            <Font name="Tahoma" size="20.0" />
         </font>
      </Button>
      <Button fx:id="bt_main_route" layoutX="407.0" layoutY="448.0" mnemonicParsing="false" onAction="#routesButtonAction" prefHeight="45.0" prefWidth="140.0" text="Route">
         <font>
            <Font name="Tahoma" size="20.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
