<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="routesSecond_pane" prefHeight="500.0" prefWidth="1000.0" style="-fx-background-color: #bdc3c7;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.RoutesSecondController">
   <children>
      <ImageView fx:id="image_routesSecond" fitHeight="150.0" fitWidth="200.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/LogoFT1.jpg" />
         </image>
      </ImageView>
      <Button fx:id="bt_close_routesSecond" layoutX="846.0" layoutY="441.0" mnemonicParsing="false" onAction="#closeroutesSecondButtonAction" prefHeight="45.0" prefWidth="140.0" text="Close">
         <font>
            <Font name="Tahoma" size="20.0" />
         </font>
      </Button>
      <Button fx:id="bt_back_routesSecond" layoutX="695.0" layoutY="441.0" mnemonicParsing="false" onAction="#backroutesSecondButtonAction" prefHeight="45.0" prefWidth="140.0" text="Back">
         <font>
            <Font name="Tahoma" size="20.0" />
         </font>
      </Button>
      <Button fx:id="bt_main_routesSecond" layoutX="544.0" layoutY="441.0" mnemonicParsing="false" onAction="#mainroutesSecondButtonAction" prefHeight="45.0" prefWidth="140.0" text="Main Menu">
         <font>
            <Font name="Tahoma" size="20.0" />
         </font>
      </Button>
      <TableView fx:id="routesSecond_table" layoutX="249.0" layoutY="54.0" prefHeight="347.0" prefWidth="659.0">
        <columns>
          <TableColumn fx:id="column_truck" prefWidth="151.0" text="Truck" />
            <TableColumn fx:id="column_driver" prefWidth="192.0" text="Driver" />
            <TableColumn fx:id="column_route" prefWidth="92.0" text="Route Number" />
            <TableColumn fx:id="column_start" prefWidth="103.0" text="Start" />
            <TableColumn fx:id="column_coming" prefWidth="101.0" text="Coming" />
        </columns>
      </TableView>
      <Button fx:id="bt_delete_routesSecond" layoutX="391.0" layoutY="441.0" mnemonicParsing="false" onAction="#deleteroutesSecondButtonAction" prefHeight="45.0" prefWidth="140.0" text="Delete">
         <font>
            <Font name="Tahoma" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
